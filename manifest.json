{
    "name": "Tiger Tiger",
    "version": "1.0",
    "description": "Tiger Trade Helper",

    "content_scripts": [
        {
            "matches": ["https://web.itiger.com/*"],
            "js": [
                "content.js"
            ],
            "run_at": "document_start",
            "all_frames": true
        }
    ],

    "manifest_version": 2,

    "web_accessible_resources": [
        "audios/*.mp3"
    ]
}